<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.corona" 
          path="/org/gnome/shell/extensions/corona/">
    
    <key type="i" name="update-interval">
      <default>30</default>
      <summary>Refresh interval in minuts</summary>
    </key>
    <key type="s" name="country">
      <default>"USA"</default>
      <summary>Selected country</summary>
    </key>
    <key type="i" name="country-id">
      <default>0</default>
      <summary>Selected country id</summary>
    </key>

  </schema>
</schemalist>
